{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-shadow": ["error", { "allow": ["data"] }],
    "max-classes-per-file": ["error", 10],
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "no-plusplus": "off",
    "no-alert": "off",
    "no-console": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}